{
  "name": "TwitterCN",
  "description": "Hide sensitive Tweets containing a content note (CN)",
  "version": "0.0.1",
  "manifest_version": 3,
  "action": {},
  "background": {
    "service_worker": "worker.js"
  },
  "permissions": [
    "tabs",
    "scripting",
    "webNavigation"
  ],
  "host_permissions": [
    "https://twitter.com/*"
  ]
}
